@{
    ViewBag.Title = "Admin Page";
}

<!DOCTYPE html>
<html>
<head>
    <!-- Site Metas -->
    <title>The Hom Pizza</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Font Awesome Icons -->
    @Styles.Render("~/cssh")
    <!-- overlayScrollbars -->
    @Styles.Render("~/cssi")
    <!-- Theme style -->
    @Styles.Render("~/cssg")
    <!-- Icon Web-->
    <link rel="shortcut icon" href="~/images/Logo.png" type="image/png">
    <style>
        div[style*="z-index: 2147483647"] {
            display: none !important;
        }
    </style>

</head>
<body class="hold-transition dark-mode sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Hello, @Session["TenKH"]</a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="~/Admin/Dashboard" class="brand-link">
                <span class="brand-text font-weight-light">The Home Pizza</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item menu-open">
                            <a href="~/Admin/Dashboard" class="nav-link">
                                <i class="nav-icon fas ion-podium"></i>
                                <p>Doanh thu</p>
                            </a>
                        </li>
                        <li class="nav-header">Product</li>
                        <li class="nav-item">
                            <a href="~/AdminCategory/Index" class="nav-link">
                                <i class="nav-icon fas ion-ios-copy"></i>
                                <p>Danh mục</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/AdminProduct/Index" class="nav-link">
                                <i class="nav-icon fas ion-ios-keypad"></i>
                                <p>Sản phẩm</p>
                            </a>
                        </li>
                        <li class="nav-header">Người dùng</li>
                        <li class="nav-item">
                            <a href="~/AdminUser/Index" class="nav-link">
                                <i class="nav-icon fas ion-person-add"></i>
                                <p>Danh sách người dùng</p>
                            </a>
                        </li>
                        <li class="nav-header">Đơn hàng</li>
                        <li class="nav-item">
                            <a href="~/AdminOrder/Index" class="nav-link">
                                <i class="nav-icon fas ion-android-cart"></i>
                                <p>Đơn hàng</p>
                            </a>
                        </li>
                        <li class="nav-header">Tác vụ</li>
                        <li class="nav-item">
                            <a href="~/User/Logout" class="nav-link">
                                <i class="fas fa-sign-out-alt"></i>
                                <p>Đăng xuất</p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        @RenderBody();
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!--JS-->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @Scripts.Render("~/jsp")
    @Scripts.Render("~/jsq")
    @Scripts.Render("~/jsr")
    @Scripts.Render("~/jst")
    @Scripts.Render("~/jso")
    @Scripts.Render("~/jsx")
    @Scripts.Render("~/jsw")
    <script>
        window.onload = function () {
            document.querySelectorAll("center a[href='http://somee.com']").forEach(el => el.remove());
            document.querySelectorAll("script[src*='somee.com/serveimages/ad2/WholeInsert5.js']").forEach(el => el.remove());
        };
    </script>
</body>
</html>
